<%- include("partials/header.ejs") %>

<div>
    <h2>Posts</h2>
    <ul class="post-list">
        <% posts.forEach(post => { %>
         <li >
            <div class="post-card">
                <h3><%= post["subject"] %></h3>
                <p><%= post["postContent"] %></p>
            </div>
        </li>
        <% }) %>
    </ul>
</div>


